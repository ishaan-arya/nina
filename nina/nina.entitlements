<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow app to send Apple events to other apps -->
    <key>com.apple.security.scripting-targets</key>
    <dict>
        <key>com.apple.TextEdit</key>
        <array>
            <string>com.apple.TextEdit</string>
        </array>
    </dict>

    <!-- Allow the app to execute other scripts -->
    <key>com.apple.security.automation.apple-events</key>
    <true/>

    <!-- Allow app to access user-selected files -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>

    <!-- Allow app to execute files that are marked as Unix executables -->
    <key>com.apple.security.files.executable</key>
    <true/>
    
    <!-- Required for most apps to interact with the network -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- If your app also needs to act as a server -->
    <key>com.apple.security.network.server</key>
    <true/>

    <!-- If your app needs to access the Downloads folder -->
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>

    <!-- If your app needs to access the Documents folder -->
    <key>com.apple.security.files.documents.read-write</key>
    <true/>
</dict>
</plist>
